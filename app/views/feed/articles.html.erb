<div class="row">
  <div class="col-md-8">
    <% if user_signed_in? %>
      <%= render partial: 'articles/form', locals: { wallable: current_user } %>
    <% end %>

    <%= render partial: 'articles/content',
               locals: { articles: @articles } %>
  </div>

  <div class="col-md-4">
    <%= render partial: 'feed/right_sidebar' %>
  </div>
</div>

<%= paginate @articles %>
